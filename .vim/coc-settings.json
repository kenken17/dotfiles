{
  "coc.preferences.formatOnType": true,
  "snippets.ultisnips.enable": true,
  "snippets.userSnippetsDirectory": "~/dotfiles/.vim/mysnippets",
  "snippets.extends": {
    "vue": ["javascript"]
  },
  "tsserver.formatOnType": false,
  "eslint.filetypes": [
    "vue",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.run": "onSave",
  "eslint.autoFixOnSave": true
}
